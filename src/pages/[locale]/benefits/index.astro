---
/**
 * Components
 */
import Layout from "@/layouts/Layout.astro";
import BenefitsPage from "@/components/Benefits/BenefitsPage";

/**
 * Utils
 */
import { getLangFromUrl, useTranslations, generateLocalizedPaths } from "@/i18n/utils";

const lang = getLangFromUrl(Astro.url);
export async function getStaticPaths() {
  const paths = await generateLocalizedPaths();
  return paths;
}
---


<Layout>
  <BenefitsPage lang={lang} client:load/>
</Layout>
